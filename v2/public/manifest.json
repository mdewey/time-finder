{
  "manifest_version": 2,
  "name": "Mirror Mirror",
  "version": "0.1",
  "description": "Show what websites you have been on today",
  "author": "Mark Dewey",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "history"
  ],
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  }
}